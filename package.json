{
  "name": "@ixiam/n8n-nodes-civicrm",
  "version": "1.1.40",
  "description": "Full-featured CiviCRM API v4 integration for n8n with support for Contacts, Memberships, Groups, Relationships, Activities and structured sub-entities like Email, Phone, and Address.",
  "license": "MIT",
  "keywords": [
    "n8n-community-node-package",
    "civicrm",
    "api4",
    "crm",
    "automation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "n8n-node dev",
    "build": "n8n-node build && pnpm run copy:assets",
    "copy:assets": "cp src/nodes/CiviCrm/civicrm.svg dist/src/nodes/CiviCrm/",
    "lint": "n8n-node lint",
    "release": "n8n-node release",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ixiam/n8n-nodes-civicrm.git"
  },
  "homepage": "https://www.ixiam.com",
  "bugs": {
    "url": "https://github.com/ixiam/n8n-nodes-civicrm/issues"
  },
  "author": "Julian Reartes <j.reartes@ixiam.com>",
  "engines": {
    "node": ">=18"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "nodes": [
      "dist/src/nodes/CiviCrm/CiviCrm.node.js"
    ],
    "credentials": [
      "dist/credentials/CiviCrmApi.credentials.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.17.0",
    "@types/node": "^20.11.30",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.6",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "overrides": {
    "eslint": "9.32.0",
    "change-case": "4.1.2"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}